#!/bin/bash
## This is a part of main script: mpm.

_ListsCount() {
    __
    echo " ${g}Default list: $(_GetLinesList -q) url(s).${nc}"
    [[ -f "$AUDIO_INFO" ]] && \
        echo " ${g}Audio list:   $(_GetLinesList -a) url(s).${nc}"
    [[ -f "$VIDEO_INFO" ]] && \
        echo " ${g}Video list:   $(_GetLinesList -v) url(s).${nc}"
}
