#!/bin/bash

_ListsCount() {
    echo ""
    echo " ${green}Default list: $(_GetLinesQueue) url(s).${nocolor}"
    [[ -f "$AUDIO_INFO" ]] && \
        echo " ${green}Audio list:   $(grep -c '^https' "$AUDIO_INFO") url(s).${nocolor}"
    [[ -f "$VIDEO_INFO" ]] && \
        echo " ${green}Video list:   $(grep -c '^https' "$VIDEO_INFO") url(s).${nocolor}"
}

