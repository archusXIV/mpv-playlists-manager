#!/bin/bash
## This is a part of main script: mpm.

# shellcheck disable=SC2154
_GetLinkTitle() {
    case "$1" in
        -a) sed -n '1p' "$AUDIO_TITLES" ;;
        -l) sed -n '1p' "$titles_dir"/"$list".titles ;;
        -q) sed -n '1p' "$tmp_dir"/DefaultTitles ;;
        -v) sed -n '1p' "$VIDEO_TITLES" ;;
    esac
}

