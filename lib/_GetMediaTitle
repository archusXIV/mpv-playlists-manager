#!/bin/bash
## This is a part of main script: mpm.

_GetMediaTitle() {
    case "$1" in
        -a) LIST="$AUDIO_INFO" ;;
        -v) LIST="$VIDEO_INFO" ;;
        -l) LIST="$list"       ;;
        -q) LIST="$QUEUE"      ;;
    esac

    mapfile -t title < <(grep '^https' "$LIST")

    for line in "${title[@]}"; do
        youtube-dl \
        --skip-download \
        --get-title \
        --no-warnings "$line" 2>/dev/null
    done
}

