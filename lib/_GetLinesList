#!/bin/bash
## This is a part of main script: mpm.

_GetLinesList() {
    case "$1" in
        -a) FILE="$AUDIO_INFO" ;;
        -c) FILE="$CHOOSE1" ;;
        -l) FILE="$playlists_dir"/"$list" ;;
        -q) FILE="$QUEUE" ;;
        -v) FILE="$VIDEO_INFO" ;;
    esac
    grep -c '^https' "$FILE"
}

