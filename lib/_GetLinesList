#!/bin/bash
## This is a part of main script: mpm.

# shellcheck disable=SC2154
_GetLinesList() {

    case "$1" in
        -a) FILE="$AUDIO_URLS" ;;
        -l) FILE="$playlists_dir"/"$list" ;;
        -q) FILE="$QUEUE" ;;
        -v) FILE="$VIDEO_URLS" ;;
    esac

    grep -c '^https' "$FILE" 2>/dev/null
    
}
